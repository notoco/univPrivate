<addons>
<addon id="script.module.urllib3" name="urllib3" version="1.25.3" provider-name="shazow">
  <requires>
    <import addon="xbmc.python" version="2.25.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">urllib3 is a powerful, sanity-friendly HTTP client for Python</summary>
    <description lang="en_GB">Packed for KODI from https://pypi.org/project/urllib3/</description>
    <platform>all</platform>
    <language></language>
    <license>MIT</license>
    <forum></forum>
    <website>https://urllib3.readthedocs.io/en/latest/</website>
    <source>https://github.com/Freso/script.module.urllib3</source>
    <email>freso.dk AT gmail.com</email>
    <assets>
      <icon>resources/icon.png</icon>
    </assets>
  </extension>
</addon>

