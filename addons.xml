		<import addon="xbmc.python" version="2.26.0" />
		<import addon="script.common.plugin.cache" version="2.5.2" />
		<import addon="script.module.myconnpy" version="1.1.7"/>
		<import addon="script.module.requests" version="2.18.4" />
